{
  "name": "parameter-space-visualizer",
  "version": "1.0.0",
  "description": "Web-based module for parameter space visualisation",
  "main": "es/index.js",
  "scripts": {
    "clean": "rimraf es && rimraf umd",
    "build": "npm run clean && cross-env NODE_ENV=production VISUALIZE_SPACE=true rollup -c",
    "dev": "rollup -c --watch",
    "dev:workers": "rollup -c rollup.config.worker.js --watch",
    "postinstall": "patch-package"
  },
  "author": {
    "name": "Martin Petr",
    "email": "martin.petr.276@gmail.com",
    "url": "http://martinpetr.cz/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mezrik/parameter-space-visualizer.git"
  },
  "homepage": "https://github.com/Mezrik/parameter-space-visualizer#readme",
  "files": [
    "LICENSE",
    "README.md",
    "es",
    "umd"
  ],
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.17.9",
    "@mantine/core": "^4.2.2",
    "@mantine/hooks": "^4.2.2",
